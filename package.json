{
  "name": "a6s-k8s-keycloak-manager",
  "version": "0.2.5",
  "description": "a6s-k8s-keycloak-manager",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "npm run clean && tsc -p tsconfig.json",
    "dev": "npm-run-all -s build -p watch:*",
    "watch:node": "nodemon ./dist/index.js",
    "watch:ts": "tsc -p tsconfig.json --watch",
    "clean": "trash dist",
    "lint:fix": "prettier \"src/**/*.ts\" --write && tslint --fix --project .",
    "lint": "tslint --project . && prettier src/**/*.ts --list-different",
    "prettier": "prettier * --write",
    "test": "nyc --reporter=html --reporter=text mocha --exit",
    "docker:cleanup": "docker-compose rm -f",
    "docker:run": "docker-compose -f docker-compose.yml up --build",
    "docker:test": "rimraf coverage/ && rimraf .nyc_output/ && rimraf mochawesome-report/ && docker-compose -f docker-compose.test.yml up --build --abort-on-container-exit",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "commit": "npx git-cz"
  },
  "author": "APIGeeks",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@fireblink/k8s-api-client": "1.1.1",
    "config": "3.1.0",
    "keycloak-admin": "1.12.0",
    "lodash": "4.17.11",
    "log4js": "4.3.1",
    "prettyjson": "1.2.1",
    "reflect-metadata": "0.1.13",
    "typedi": "0.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "@types/chai": "4.1.7",
    "@types/config": "0.0.34",
    "@types/lodash": "4.14.134",
    "@types/mocha": "5.2.7",
    "@types/node": "12.0.7",
    "@types/prettyjson": "0.0.29",
    "chai": "4.2.0",
    "commitizen": "3.1.1",
    "conventional-changelog-cli": "2.0.21",
    "cz-conventional-changelog": "2.1.0",
    "husky": "4.2.4",
    "jsonlint": "1.6.3",
    "lint-staged": "8.2.0",
    "mocha": "6.1.4",
    "mochawesome": "4.0.0",
    "nodemon": "1.19.1",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "prettier": "1.18.0",
    "trash-cli": "3.0.0",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "6.0.0",
    "typescript": "3.5.1",
    "yaml-lint": "1.2.4"
  },
  "keywords": []
}
