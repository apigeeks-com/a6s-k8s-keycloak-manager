{
  "name": "a6s-k8s-keycloak-manager",
  "version": "0.2.2",
  "description": "a6s-k8s-keycloak-manager",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "npm run clean && tsc -p tsconfig.json",
    "dev": "npm-run-all -s build -p watch:*",
    "watch:node": "nodemon ./dist/index.js",
    "watch:ts": "tsc -p tsconfig.json --watch",
    "clean": "trash dist",
    "lint:fix": "prettier \"src/**/*.ts\" --write && tslint --fix --project .",
    "lint": "tslint --project . && prettier src/**/*.ts --list-different",
    "prettier": "prettier * --write",
    "docker:cleanup": "docker-compose rm -f",
    "docker:run": "docker-compose -f docker-compose.yml up --build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "commit": "npx git-cz"
  },
  "author": "APIGeeks",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@fireblink/k8s-api-client": "^1.0.1",
    "config": "^3.0.1",
    "keycloak-admin": "^1.10.0",
    "lodash": "^4.17.11",
    "log4js": "^4.0.2",
    "prettyjson": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/config": "^0.0.34",
    "@types/lodash": "^4.14.123",
    "@types/node": "^10.12.10",
    "@types/prettyjson": "^0.0.28",
    "commitizen": "^3.0.5",
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.2.0",
    "jsonlint": "^1.6.3",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.2",
    "trash-cli": "^1.4.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-immutable": "^5.0.0",
    "typescript": "^3.1.6",
    "yaml-lint": "^1.2.4"
  },
  "keywords": []
}
